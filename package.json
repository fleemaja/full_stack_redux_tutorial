  "bugs": {
    "url": "https://github.com/fleemaja/full_stack_redux_tutorial/issues"
  },
  "homepage": "https://github.com/fleemaja/full_stack_redux_tutorial",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "jsdom": "^9.10.0",
    "mocha": "^3.2.0",
    "react-hot-loader": "^1.3.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^2.0.0",
    "redux": "^3.6.0",
    "socket.io": "^1.7.2"
  }
}
