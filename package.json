{
  "name": "full_stack_redux_tutorial",
  "version": "1.0.0",
  "description": "I am following along with [Full-Stack Redux Tutorial](https://teropa.info/blog/2015/09/10/full-stack-redux-tutorial.html \"Full-Stack Redux Tutorial\") to learn full-stack Redux app development and to get comfortable with developing with strictly immutable data structures.",
  "main": "index.js",
  "scripts": {
    "start": "babel-node index.js",
    "serverTest": "mocha --compilers js:babel-core/register --require ./serverTest/test_helper.js  --recursive",
    "clientTest": "mocha --compilers js:babel-core/register --require ./clientTest/test_helper.js \"clientTest/**/*@(.js|.jsx)\"",
    "serverTest:watch": "npm run serverTest -- --watch",
    "clientTest:watch": "npm run clientTest -- --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fleemaja/full_stack_redux_tutorial.git"
  },
  "keywords": [],
